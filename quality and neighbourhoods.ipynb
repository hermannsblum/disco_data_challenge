  {
   "cell_type": "code",
   "execution_count": 43,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "means = supercats_frame.groupby(['super_category', 'sub_category'])['stars'].mean()\n",
    "stds = supercats_frame.groupby(['super_category', 'sub_category'])['stars'].std()\n",
    "factor = 1.4\n",
    "\n",
    "def threshold(row):\n",
    "    sup = row['super_category']\n",
    "    sub = row['sub_category']\n",
    "    if row['stars'] > means[(sup,sub)] + factor * stds[(sup,sub)]:\n",
    "        return \"good\"\n",
    "    elif row['stars'] < means[(sup,sub)] - factor * stds[(sup,sub)]:\n",
    "        return \"bad\"\n",
    "    else:\n",
    "        return \"semi\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "frame = supercats_frame.copy(deep=True)\n",
    "frame['quality'] = frame.apply(lambda row: threshold(row), axis=1)"
   ]
